{
  "name": "webdriverio-automation-testing-telnyx",
  "version": "1.0.0",
  "description": "Project for automated testing with WebdriverIO",
  "main": "index.js",
  "scripts": {
  "clean:allure": "rm -rf allure-results",
  "test": "npm run clean:allure && wdio run ./config/wdio.conf.ts",
  "test:chrome": "CHROME=true npm run test",
  "test:firefox": "FIREFOX=true npm run test:env",
  "test:file": "npm run clean:allure && wdio run ./config/wdio.conf.ts --spec",
  "allure:generate": "allure generate allure-results --clean -o allure-report",
  "allure:open": "allure open allure-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HoliakDenys/webdriverio-automation-testing-telnyx.git"
  },
  "keywords": [
    "webdriverio",
    "typescript",
    "allure"
  ],
  "author": "Denys Holiak (https://github.com/HoliakDenys)",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/HoliakDenys/webdriverio-automation-testing-telnyx/issues"
  },
  "homepage": "https://github.com/HoliakDenys/webdriverio-automation-testing-telnyx#readme",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.12.3",
    "@wdio/cli": "^9.12.6",
    "@wdio/local-runner": "^9.12.5",
    "@wdio/mocha-framework": "^9.12.5",
    "@wdio/types": "^9.12.6",
    "dotenv": "^16.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
